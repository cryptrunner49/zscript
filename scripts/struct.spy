// Emoji Examples
struct 🐱
var kitty = 🐱()
kitty.name = "Whiskers"
kitty.age = 2
println(kitty.name)  // Outputs: Whiskers

struct 🐶:
    breed = "Labrador"
    age = 3
    color = "Golden"

var buddy = 🐶()
println(buddy.breed)  // Outputs: Labrador

struct 🐰
var thumper = 🐰()
thumper.favoriteFood = "Carrots"
println(thumper.favoriteFood)  // Outputs: Carrots

// ASCII Examples
struct Cat
var kitten = Cat()
kitten.name = "Tama"
kitten.age = 2
println(kitten.name)  // Outputs: Tama

struct Dog:
    breed = "Labrador"
    age = 3
    
var pup = Dog()
println(pup.breed)  // Outputs: Labrador

struct Bunny
var bunny = Bunny()
bunny.favoriteFood = "Carrot"
println(bunny.favoriteFood)  // Outputs: Carrot

struct Test:
    x = 1
    y = "text"
    z = true
    w = null
    xy = [1, 2]
    xz = {"1":"test"}

var test = Test()
println(test.x)
println(test.y)
println(test.z)
println(test.w)
println(test.xy)
println(test.xz)